<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app
  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
>
  <display-name>Archetype Created Web Application</display-name>

  <welcome-file-list>
    <welcome-file>travel</welcome-file>
  </welcome-file-list>



  <servlet>
    <servlet-name>cart</servlet-name>
    <servlet-class>demo.control.CartServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>cart</servlet-name>
    <url-pattern>/demo/cart</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>travel</servlet-name>
    <servlet-class>demo.control.TravelServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>travel</servlet-name>
    <url-pattern>/demo/travel</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>demo.control.LoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/demo/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>demo.control.LogoutServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/demo/logout</url-pattern>
  </servlet-mapping>

  
</web-app>
